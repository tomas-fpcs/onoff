<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
        "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN"
        "https://checkstyle.org/dtds/configuration_1_3.dtd">

<module name="Checker">
    <module name="TreeWalker">
        <!-- Enable Google Checks (manually added instead of importing) -->
        <module name="JavadocType">
            <property name="severity" value="ignore"/>  <!-- Disable Javadoc class checks -->
        </module>
        <module name="JavadocMethod">
            <property name="severity" value="ignore"/>  <!-- Disable Javadoc method checks -->
        </module>
        <module name="JavadocVariable">
            <property name="severity" value="ignore"/>  <!-- Disable Javadoc variable checks -->
        </module>
    </module>
</module>
